package org.rdswitchboard.libraries.graph;

import java.util.HashMap;
import java.util.HashSet;
import java.util.Map;
import java.util.Set;

/**
 * Class to store relationship information
 * 
 * @author Dima Kudriavcev (dmitrij@kudriavcev.info)
 * @date 2015-07-11
 * @version 1.0.0
 *
 * 
 */

public class Relationship extends Record {

	/**
	 * Default class constructor
	 */
	public Relationship() {
	}
	
	/**
	 * Relationship constructor
	 * @param keyFrom From Key
	 * @param keyTo To key
	 * @param type Type name
	 */
	public Relationship(String key, String type) {
		super(key, type);
	}
	
	/**
	 * Alternative Relationship constructor
	 * @param keyFrom From Key
	 * @param keyTo To key
	 * @param type Type name
	 * @param properties Additional properties (if null, please use other constructor)
	 */
	
	public Relationship(String key, String type, Map<String, Object> properties) {
		super(key, type, properties);
	}
	
	@Override
	public String toString() {
		return "Relationship " + properties;
	}
}
